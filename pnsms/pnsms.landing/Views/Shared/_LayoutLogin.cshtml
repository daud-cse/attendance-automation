
<!DOCTYPE html>
<html>



<head>

    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>@ViewBag.Title</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">   
    <link rel="shortcut icon" href="~/favicon1.ico" />
    <link rel="apple-touch-icon-precomposed" href="~/assets/img/favicon/apple-touch-icon.png">

    <!-- Fonts -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Oxygen|Source+Sans+Pro:400,600">
    <link rel="stylesheet" href="~/assets/css/iconmoon.css">

    <!-- Stylesheet -->
    <link rel="stylesheet" href="~/assets/css/responsive_theme.css">
    <link rel="stylesheet" href="~/assets/css/theme_style.css">
    <link rel="stylesheet" href="~/assets/css/theme_styles.css">

    <link rel="stylesheet" href="~/assets/css/prettyPhoto.css">
    <link rel="stylesheet" href="~/assets/css/owl.carousel.css">
    <link rel="stylesheet" href="~/assets/css/flexslider.css">
    <link rel="stylesheet" href="~/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/assets/css/normalize.css">
    <link rel="stylesheet" href="~/assets/css/common.css">
    <link rel="stylesheet" href="~/assets/css/style.css">
    <link rel="stylesheet" href="~/assets/css/responsive.css">
    <link href="~/Content/fullcalendar.css" rel="stylesheet" />

    <link href="~/Content/themes/jquery.ui.all.css" rel="stylesheet" />   

</head>
<body>
    <style type="text/css">
       

        .wrapper.wrapper_boxed {
           
            max-width: 100%;
            width:100%;
            border-top:0px solid #1f1f1f;
        }
       

        }
        .tab-nav {
            width: 370px;
        }
      
    </style>
    <div class="wrapper wrapper_boxed">
        <span class="label label-danger" id="disconnected"></span>

        @Html.Partial("_headerLogin")


        @RenderBody()


        @Html.Partial("_footerLogin")
    </div>
    @*@Scripts.Render("~/bundles/bootstrap")*@

    
    <!-- Scripts -->

    @*<script src="https://code.jquery.com/jquery-1.12.0.min.js"></script>*@
    @*<script>window.jQuery || document.write('<script src="~/assets/js/vendor/jquery-1.12.0.min.js"><\/script>')</script>*@
    
    <script src="~/assets/js/vendor/modernizr-2.8.3.min.js"></script>
    <script src="~/assets/js/vendor/jquery-2.2.4.min.js"></script>
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/jquery.flexslider-min.js"></script>
    <script src="~/assets/js/owl.carousel.min.js"></script>
    <script src="~/assets/js/jquery.prettyPhoto.js"></script>
    <script src="~/assets/js/scripts.js"></script>
    <script src="~/assets/js/vendor/waypoints.min.js"></script>
    <script src="~/assets/js/vendor/jquery.counterup.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.10.4.min.js"></script>
    <script src="~/Scripts/fullcalendar.min.js"></script>
    

    <script type="text/javascript">
        setInterval("keepSessionAlive()", 290000);//5000

        function keepSessionAlive() {
            $.ajax({
                type: 'POST',
                url: '@Url.Content("~/Home/keepSessionAlive")',
                success: function () {
                    $("#disconnected").html("");
                },
                error: function () {
                    //alert('You are disconnected');
                    $("#disconnected").html("You are disconnected");
                }
            });
        }



      

        //$("#document").ready(function () {

        //    $("html").niceScroll({ cursorwidth: '10px', autohidemode: true, zindex: 999 });
        //});
    </script>

    @RenderSection("scripts", required: false)




</body>
</html>
